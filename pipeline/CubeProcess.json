						"Type": "Full",
						"CommitMode": "transactional",
						"MaxParallelism": 2,
						"RetryCount": 2,
						"Objects": [
							{
								"database": "PG_DMS",
								"table": "tblFactCFR",
								"partition": "PartitionCurrentQuarter_FactCFRData"
							},
